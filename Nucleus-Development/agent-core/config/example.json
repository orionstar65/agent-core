{
  "backend": {
    "baseUrl": "https://api.nucleus.example",
    "authPath": "/deviceservices/api/Authentication/devicecertificatevalid/",
    "isRegisteredPath": "/deviceservices/api/devicemanagement/isdeviceregistered/",
    "getActivationPath": "/deviceservices/api/devicemanagement/getactivationinformation/"
  },
  "identity": {
    "isGateway": false,
    "deviceSerial": "",
    "gatewayId": "",
    "uuid": ""
  },
  "tunnelInfo": {
    "enabled": false
  },
  "mqtt": {
    "host": "mqtt.nucleus.example",
    "port": 8883,
    "keepalive": 30
  },
  "cert": {
    "storeHint": "OS",
    "certPath": "./cert_base64.txt"
  },
  "retry": {
    "maxAttempts": 5,
    "baseMs": 500,
    "maxMs": 8000
  },
  "resource": {
    "cpuMaxPct": 60,
    "memMaxMB": 512,
    "netMaxKBps": 256
  },
  "logging": {
    "level": "info",
    "json": true
  },
  "ssm": {
    "agentPath": "/snap/amazon-ssm-agent/current/amazon-ssm-agent"
  }
}
