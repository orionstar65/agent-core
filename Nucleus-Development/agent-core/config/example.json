{
  "backend": {
    "baseUrl": "https://api.nucleus.example",
    "authPath": "/deviceservices/api/Authentication/devicecertificatevalid/",
    "isRegisteredPath": "/deviceservices/api/devicemanagement/isdeviceregistered/",
    "getActivationPath": "/deviceservices/api/devicemanagement/getactivationinformation/"
  },
  "identity": {
    "isGateway": false,
    "deviceSerial": "",
    "gatewayId": "",
    "uuid": ""
  },
  "tunnelInfo": {
    "enabled": false
  },
  "mqtt": {
    "host": "mqtt.nucleus.example",
    "port": 8883,
    "keepalive": 30
  },
  "cert": {
    "storeHint": "OS",
    "certPath": "./cert_base64.txt"
  },
  "retry": {
    "maxAttempts": 5,
    "baseMs": 500,
    "maxMs": 8000
  },
  "resource": {
    "cpuMaxPct": 60,
    "memMaxMB": 512,
    "netMaxKBps": 256
  },
  "logging": {
    "level": "info",
    "json": true
  },
  "ssm": {
    "agentPath": "/snap/amazon-ssm-agent/current/amazon-ssm-agent"
  },
  "service": {
    "maxRestartAttempts": 5,
    "restartBaseDelayMs": 1000,
    "restartMaxDelayMs": 300000,
    "restartJitterFactor": 0.2,
    "quarantineDurationS": 3600
  },
  "zmq": {
    "pubPort": 5555,
    "reqPort": 5556,
    "curveEnabled": false,
    "curveServerKey": "",
    "curvePublicKey": "",
    "curveSecretKey": ""
  },
  "extensions": {
    "manifestPath": "manifests/extensions.json",
    "maxRestartAttempts": 3,
    "restartBaseDelayMs": 1000,
    "restartMaxDelayMs": 60000,
    "quarantineDurationS": 300,
    "healthCheckIntervalS": 30,
    "crashDetectionIntervalS": 5
  }
}
